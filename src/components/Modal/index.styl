.Modal
  background-color rgba(0,0,0,0.9)
  display grid
  justify-content center
  height 100vh
  left 0
  margin 0
  place-items center center
  position absolute
  top 0
  width 100%
  visibility hidden

.ModalContainer
  background-color white
  padding 5rem 8rem

.isVisible
  visibility visible